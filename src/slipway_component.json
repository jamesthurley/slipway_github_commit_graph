{
  "publisher": "jamesthurley",
  "name": "github_commit_graph",
  "version": "1.0.0",
  "input": {
    "properties": {
      "width": { "type": "uint32" },
      "height": { "type": "uint32" },
      "username": { "type": "string" }
    },
    "optionalProperties": {
      "github_token": { "type": "string" },
      "horizontal_padding": { "type": "uint32" },
      "vertical_padding": { "type": "uint32" },
      "background_color": { "type": "string" },
      "font_color": { "type": "string" },
      "day_border_width": { "type": "uint32" },
      "day_border_color": { "type": "string" },
      "day_palette": { "elements": { "type": "string" } }
    }
  },
  "output": "canvas",
  "rigging": {
    "output": {
      "component": "slipwayhq.jsx.0.5.2",
      "allow": [
        { "permission": "registry_components" },
        { "permission": "fonts" }
      ],
      "input": {
        "width": "$$.input.width",
        "height": "$$.input.height",
        "data": "$$.input.run.data",
        "jsx": "$$.input.run.jsx",
        "background_color": "$$?input.background_color"
      }
    }
  }
}